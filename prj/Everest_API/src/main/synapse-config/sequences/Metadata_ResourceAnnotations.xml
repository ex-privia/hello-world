<?xml version="1.0" encoding="UTF-8"?>
<sequence name="Metadata_ResourceAnnotations" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <iterate continueParent="true" expression="$ctx:BODY_FOR_RESOURCES//internal | $ctx:BODY_FOR_RESOURCES//external">
