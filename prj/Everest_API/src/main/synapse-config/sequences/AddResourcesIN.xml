            <property name="HTTP_SC" scope="axis2" type="STRING" value="401"/>
            <property name="messageType" scope="axis2" type="STRING" value="application/xml"/>
            <script language="js"><![CDATA[//
				    mc.getEnvelope().getBody().getFirstElement().detach();
				   //]]></script>
